{
  "name": "algolia-codegen",
  "version": "0.1.1",
  "description": "Generate TypeScript types from Algolia indices",
  "license": "MIT",
  "author": "nightlightmare",
  "repository": {
    "type": "git",
    "url": "https://github.com/nightlightmare/algolia-codegen.git"
  },
  "homepage": "https://github.com/nightlightmare/algolia-codegen#readme",
  "bugs": {
    "url": "https://github.com/nightlightmare/algolia-codegen/issues"
  },
  "keywords": [
    "algolia",
    "typescript",
    "codegen",
    "type-generation",
    "algolia-types",
    "code-generation",
    "typescript-generator"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "algolia-codegen": "dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm clean && pnpm build"
  },
  "dependencies": {
    "algoliasearch": "^4.24.0",
    "commander": "^12.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}